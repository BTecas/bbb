#~ expand ;

option solver cplexamp;
option cplex_options 'sensitivity';
solve;
display MO_max ; #, s_cultivos_w, s_cultivos_x, s_cultivos_y, s_cultivos_z ;

####
#Variables: VAlor actual (val), Costo reducido(rc), Cota superior (ub), Cota inferior (lb) 

#~ display s_cultivos_w.val, s_cultivos_w.rc, s_cultivos_w.ub, s_cultivos_w.lb;# Cuartel W
#~ display s_cultivos_x.val, s_cultivos_x.rc, s_cultivos_x.ub, s_cultivos_x.lb;#Cuartel x
#~ display s_cultivos_y.val, s_cultivos_y.rc, s_cultivos_y.ub, s_cultivos_y.lb;#Cuartel y
#~ display s_cultivos_z.val, s_cultivos_z.rc, s_cultivos_z.ub, s_cultivos_z.lb;#Cuartel z

#####################
# Restricciones 
#~ display S_max_w.body,  S_max_w.dual, S_max_w.slack,  S_max_w.ub, S_max_w.lb;# restriccion 1, superficie w
#~ display S_max_x.body,  S_max_x.dual, S_max_x.slack,  S_max_x.ub, S_max_x.lb;# restriccion 2, superficie x
#~ display S_max_y.body,  S_max_y.dual, S_max_y.slack,  S_max_y.ub, S_max_y.lb;# restriccion 3, superficie y
#~ display S_max_z.body,  S_max_z.dual, S_max_z.slack,  S_max_z.ub, S_max_z.lb;# restriccion 4, superficie z
#~ expand S_max_w; 
#~ display s_cultivos_z ;
#~ display S_max_z.body,  S_max_z.dual, slackY, S_max_z.slack,  s_max_z;# restriccion 4, superficie z
#~ display MO_max.body,  MO_max.dual, MO_max.slack,  MO_max.ub, MO_max.lb;# restriccion 5, MO_max
#~ display IN_min.body,  IN_min.dual, IN_min.slack,  IN_min.ub, IN_min.lb;# restriccion 6, IN_min
####


