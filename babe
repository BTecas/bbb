#########
#los datos que tiene el modelo son  los datos que entrego  la agricultora (original)
#tiene  10 restriciones (4 de superficie,una de mano de obra, una deingreso minimo y 4 de cultivos no asignados relacionado con los bloques )
#modelo_1
########


#se definen 5 set, uno corresponde a los meses y los otros  a cada cuartel
set mes;#[i]
set cultivo_cw;	#cultivos en el cuartel 1 (1=w) [j]
set cultivo_cx;	#cultivos en el cuartel 2 (2=x) [k]
set cultivo_cy;	#cultivos en el cuartel 3 (3=y) [l]
set cultivo_cz;	#cultivos en el cuartel 4 (4=z) [m]



# Se hacen los links de cada cuartel 
set links_cw within {mes, cultivo_cw};
set links_cx within {mes, cultivo_cx};
set links_cy within {mes, cultivo_cy};
set links_cz within {mes, cultivo_cz};


# Esto corresponde al parametro que va al lado derecho  de la restrición
param s_max_w {i in mes};  	#superficie maxima por mes,cuartel W 
param s_max_x {i in mes};  	#superficie maxima por mes,cuartel X
param s_max_y {i in mes}; 	#superficie maxima por mes,cuartel Y
param s_max_z {i in mes};	#superficie maxima por mes,cuartel Z
param mo_max {i in mes};		#Mano de Obra maxima por mes,cuartel W,X,Y,Z
param ing_min {i in mes};	#Ingreso Minimo por mes,cuartel W,X,Y,Z
param cos_max {i in mes};	#costo máximo por mes, cuartel W,X,Y,Z
param S_min_w {j in cultivo_cw}; # corresponde a la superficie minima que se considera para los cultivos del bloque w durante el primer y segundo año
param S_min_x {k in cultivo_cx}; # corresponde a la superficie minima que se considera para los cultivos del bloque x durante el primer y segundo año
param S_min_y {l in cultivo_cy}; # corresponde a la superficie minima que se considera para los cultivos del bloque y durante el primer y segundo año
param S_min_z {m in cultivo_cz}; # corresponde a la superficie minima que se considera para los cultivos del bloque z durante el primer y segundo año


#corresponde al mes en el cual el cultivo está activo
#Se ocupará en las Restricciones N°1, N°2, N°3 y N°4
param tab_superficie_w {links_cw};		#Tabla Superficie del cuartel W
param tab_superficie_x {links_cx};		#Tabla Superficie del cuartel X
param tab_superficie_y {links_cy};		#Tabla Superficie del cuartel Y
param tab_superficie_z {links_cz};		#Tabla Superficie del cuartel Z

#corresponde a la mano de obra empleada por mes
#Se ocupará en la Restricción N°5
param tab_mo_w {links_cw};	#Tabla Mano de Obra necesaria por mes en el cuartel W
param tab_mo_x {links_cx};	#Tabla Mano de Obra necesaria por mes en el cuartel X		
param tab_mo_y {links_cy};	#Tabla Mano de Obra necesaria por mes en el cuartel Y
param tab_mo_z {links_cz};	#Tabla Mano de Obra necesaria por mes en el cuartel Z

#corresponde al ingreso  durante el ultimo mes (mes de cosecha)
#Se ocupará en la Restricción N°6
param tab_ing_w {links_cw};	#Tabla de Ingresos monetario en el mes cosechado, cuartel W
param tab_ing_x {links_cx};	#Tabla de Ingresos monetario en el mes cosechado, cuartel X
param tab_ing_y {links_cy};	#Tabla de Ingresos monetario en el mes cosechado, cuartel Y
param tab_ing_z {links_cz};	#Tabla de Ingresos monetario en el mes cosechado, cuartel Z


var s_cultivos_w {cultivo_cw} >= 0; #Superficie del cultivo seleccionado, cuartel W 
var s_cultivos_x {cultivo_cx} >= 0;  #Superficie del cultivo seleccionado, cuartel x
var s_cultivos_y {cultivo_cy} >= 0;  #Superficie del cultivo seleccionado, cuartel y 
var s_cultivos_z {cultivo_cz} >= 0;  #Superficie del cultivo seleccionado, cuartel z
var slackY {i in mes} >=0 ;
param e = 1 ;

maximize Total_ingresos: #el producto de la superficie del cultivo por los ingresos
	sum {(i,j) in links_cw} s_cultivos_w [j] * tab_ing_w [i,j] + 
	sum {(i,k) in links_cx} s_cultivos_x [k] * tab_ing_x [i,k] + 
	sum {(i,l) in links_cy} s_cultivos_y [l] * tab_ing_y [i,l] + 
	sum {(i,m) in links_cz} s_cultivos_z [m] * tab_ing_z [i,m] +
	e * sum{i in mes} slackY [i] 
	;
	
#restricción superficie por cuartel w N°1
subject to S_max_w {i in mes}:
	0 <= sum {(i,j) in links_cw} s_cultivos_w [j] * tab_superficie_w [i,j] <= s_max_w [i];
	
#restricción superficie por cuartel x  N°2	
subject to S_max_x {i in mes}:
	0 <= sum {(i,k) in links_cx} s_cultivos_x [k] * tab_superficie_x [i,k] <= s_max_x [i];
	
#restricción superficie por cuartel y N°3	
subject to S_max_y {i in mes}:
	0 <= sum {(i,l) in links_cy} s_cultivos_y [l] * tab_superficie_y [i,l] <= s_max_y [i];
	
#restricción superficie por cuartel z N°4	
subject to S_max_z {i in mes}:
	 sum {(i,m) in links_cz} s_cultivos_z [m] * tab_superficie_z [i,m] + slackY[i] = s_max_z [i];

#restricción N°5 mano de obra disponible mensual 
subject to MO_max {i in mes}:
	sum {(i,j) in links_cw} s_cultivos_w [j] * tab_mo_w [i,j] + 
	sum {(i,k) in links_cx} s_cultivos_x [k] * tab_mo_x [i,k] + 
	sum {(i,l) in links_cy} s_cultivos_y [l] * tab_mo_y [i,l] + 
	sum {(i,m) in links_cz} s_cultivos_z [m] * tab_mo_z [i,m] <= mo_max [i];

#~ #restricción N°6, ingreso minimo esperado	
subject to IN_min {i in mes}:
	sum {(i,j) in links_cw} s_cultivos_w [j] * tab_ing_w  [i,j] + 
	sum {(i,k) in links_cx} s_cultivos_x [k] * tab_ing_x [i,k] + 
	sum {(i,l) in links_cy} s_cultivos_y [l] * tab_ing_y [i,l] + 
	sum {(i,m) in links_cz} s_cultivos_z [m] * tab_ing_z [i,m] >= ing_min [i];
	
	
###############	cultivos No asignados 

#~ #desde la restriccion 7 hasta la 10 corresponden a los cultivos no asignados en relacion a su cuartel

#~ #restriccion N°7
subject to Cult_no_asig_w {j in cultivo_cw}:
	s_cultivos_w [j] >= S_min_w [j];

#restriccion N°8
subject to Cult_no_asig_x {k in cultivo_cx}:
	s_cultivos_x [k] >= S_min_x [k];

#restriccion N°9
subject to Cult_no_asig_y {l in cultivo_cy}:
	s_cultivos_y [l] >= S_min_y [l];

#restriccion N°10
subject to Cult_no_asig_z {m in cultivo_cz}:
	s_cultivos_z [m] >= S_min_z [m];
			
			######
	

data; 

set mes:= ene_1	feb_1	mar_1	abr_1	may_1	jun_1	jul_1	ago_1	sept_1	oct_1	nov_1	dic_1	ene_2	feb_2	
		mar_2	abr_2	may_2	jun_2	jul_2	ago_2	sept_2	oct_2	nov_2	dic_2;
		
set cultivo_cw:= 
			AC1	AC2	AC3	AC8	AC9	AC10	AC11	AC12	AC13	AC14	AC15	AC21	AC22	AC23	AC24	CB8	CB9	CB10	CB20	CB21	CB22	CI8	CI9	CI10	CI11	CI20	CI21	CI22	CI23	ES2	ES3	ES4	ES5	ES6	ES7	ES8
			ES9	ES10	ES11	ES14	ES15	ES16	ES17	ES18	ES19	ES20	ES21	ES22	ES23	LE1	LE2	LE3	LE4	LE5	LE6	LE7	LE8	LE9	LE10	LE11	LE12	LE13	LE14	LE15	LE16	LE17	LE18	LE19	LE20	LE21	LE22	LE23
			PE1	PE2	PE3	PE4	PE5	PE6	PE7	PE8	PE9	PE10	PE11	PE12	PE13	PE14	PE15	PE16	PE17	PE18	PE19	PE20	PE21	PO1	PO10	PO11	PO12	PO13	PO14	PO22	TO8	TO9	TO10	TO11	TO20	TO21	TO22	;
			
set cultivo_cx:=
			XBE8	XBE9	XBE10	XBE20	XBE21	XBE22	XLE1	XLE2	XLE8	XLE9	XLE10	XLE11	XLE12	XLE13	XLE14	XLE20	XLE21	XLE22	XTO10	XTO11	XTO12	XTO22	XZI10	XZI11	XZI12	XZI22	;
			
set cultivo_cy:= 
			YBE8	YBE9	YBE10	YBE20	YBE21	YBE22	YLE1	YLE2	YLE8	YLE9	YLE10	YLE11	YLE12	YLE13	YLE14	YLE20	YLE21	YLE22	YTO10	YTO11	YTO12	YTO22	YZI10	YZI11	YZI12	YZI22	;
			
set cultivo_cz:= 
			ZBE8	ZBE9	ZBE10	ZBE20	ZBE21	ZBE22	ZLE1	ZLE2	ZLE8	ZLE9	ZLE10	ZLE11	ZLE12	ZLE13	ZLE14	ZLE20	ZLE21	ZLE22	ZTO10	ZTO11	ZTO12	ZTO22	ZZI10	ZZI11	ZZI12	ZZI22	;


set links_cw (tr):	
		ene_1	feb_1	mar_1	abr_1	may_1	jun_1	jul_1	ago_1	sept_1	oct_1	nov_1	dic_1	ene_2	feb_2	mar_2	abr_2	may_2	jun_2	jul_2	ago_2	sept_2	oct_2	nov_2	dic_2:=
	AC1	+	+	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-
	AC2	-	+	+	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-
	AC3	-	-	+	+	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-	-
	AC8	-	-	-	-	-	-	-	+	+	+	-	-	-	-	-	-	-	-	-	-	-	-	-	-
	AC9	-	-	-	-	-	-	-	-	+	+	+	-	-	-	-	-	-	-	-	-	-	-	-	-
	AC10	-	-	-	-	-	-	-	-	-	+	+	-	-	-	-	-	-	-	-	-	-	-	-	-
